{% extends 'layouts/base.twig'%}

{% block main_content %}
	<h2>About Image #
		{{ image.id }}
	</h2>
	<ul style="list-style: none;">
		<li>
			<label>Assigned Variant:</label>
			{{ variant.title }}
		</li>
		<li>
			<label>ID:</label>
			{{ image.id }}
		</li>
	</ul>
	<p>
		<a href="/images/{{image.id}}/edit">Edit</a>
		|
		<a href="/images/{{image.id}}/delete">Delete</a>
	</p>
{% endblock %}